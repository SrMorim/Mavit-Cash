{
  "name": "mavit-cash",
  "version": "1.0.0",
  "description": "Mavit - Cash: Elegant desktop financial management application",
  "main": "dist-electron/main.js",
  "homepage": ".",
  "scripts": {
    "dev": "npm run build:electron && concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:electron": "electron-forge start",
    "dev:react": "vite",
    "build": "npm run clean && npm run build:react && npm run build:electron && npm run make",
    "build:react": "vite build",
    "build:electron": "tsc --project tsconfig.electron.json",
    "make": "electron-forge make",
    "clean": "rm -rf dist dist-electron out",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "debug:linux": "npm run build && ./out/Mavit\\ Cash-linux-x64/mavit-cash"
  },
  "keywords": ["electron", "react", "typescript", "finance", "budget", "personal-finance"],
  "author": "Maverick",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-fuses": "^7.2.0",
    "@electron/fuses": "^1.6.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "electron-store": "^8.1.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.302.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.2.0",
    "zustand": "^4.4.7"
  }
}